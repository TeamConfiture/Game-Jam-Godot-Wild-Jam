# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"
var num = 0
var launched = false
var damage = 0.0
var d = 0
# Called when the node enters the scene tree for the first time.
func _ready():
	damage = 0.0
	launched = false
	num = 0
	d = 0
	pass



# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	#print(\"len\" + str(len(get_children())))
	if num < len(get_children()):
		if (get_child(num).position.x < 90 && !launched):
			get_child(num).position.x += 3
		elif get_child(num).position.x > -150 && launched:
			get_child(num).position.x -= 3
		elif (get_child(num).position.x >= 90):
			get_node(\"../Dice\").visible = true
			if (d > 50): # Make the program sleeping to keep cats in the same point for 50 frames
				#print(get_node(\"../Dice\").visible)
				damage += rand_range(1,20)
				get_node(\"../Dice\").visible = false
				launched = true
				get_child(num).position.x -= 3
			d += 1
		else:
			launched = false
			num += 1
			d = 0
		#print(get_child(num).position.x)
	#print(damage) 
